<% provide(:title, 'Sign in') %>
	<div class = "form">
	<h1> Sign in </h1>
	<%= form_for(:session, url: sessions_path) do |f| %>
		
		<%= render "shared/error_mesages", object: f.object %>

		<%= f.label :last_name %>
		<%= f.text_field :last_name %>

		<%= f.label :password %>
		<%= f.password_field :password %>

		<%= f.submit 'Sign in', class: 'btn' %>
	<% end %>
</div>
